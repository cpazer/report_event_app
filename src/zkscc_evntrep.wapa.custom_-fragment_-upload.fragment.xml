<core:FragmentDefinition xmlns:commons="sap.ui.commons" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form" xmlns:l="sap.ui.layout"                                                                                                                       
	xmlns:sfi="sap.ui.comp.smartfield" xmlns:sfo="sap.ui.comp.smartform" xmlns:table="sap.ui.table" xmlns:smartTable="sap.ui.comp.smarttable"                                                                                                                     
	xmlns:template="http://schemas.sap.com/sapui5/extension/sap.ui.core.template/1" xmlns:uxap="sap.uxap" xmlns="sap.m"                                                                                                                                           
	xmlns:upload="sap.m.upload">                                                                                                                                                                                                                                  
	<upload:UploadSet core:require="{handler: 'onpremreportevent/custom/UploadSetController'}"&nbsp;                                                                                                                                                              
        id="UploadSet" instantUpload="true" showIcons="false" uploadEnabled="true" terminationEnabled="true"  visible="true"                                                                                                                                   
        beforeUploadStarts="handler.uploadAttachment" afterItemRemoved="handler.removeAttachment"                                                                                                                                                              
        items="{path: '_media', templateShareable: true}">                                                                                                                                                                                                     
		<upload:items id="UploadItems">                                                                                                                                                                                                                              
			<upload:UploadSetItem fileName="{Filename}" mediaType="{Mimetype}" url="{Mediauuid}" openPressed="handler.onOpenPressed">                                                                                                                                   
				<upload:attributes>                                                                                                                                                                                                                                        
					<ObjectAttribute title="Uploaded on" text="{path:'Sydate'}"/>                                                                                                                                                                                             
				</upload:attributes>                                                                                                                                                                                                                                       
			</upload:UploadSetItem>                                                                                                                                                                                                                                     
		</upload:items>                                                                                                                                                                                                                                              
	</upload:UploadSet>                                                                                                                                                                                                                                           
</core:FragmentDefinition>                                                                                                                                                                                                                                     