sap.ui.define([],                                                                                                                                                                                                                                              
function (){                                                                                                                                                                                                                                                   
    "use strict";                                                                                                                                                                                                                                              
    return {                                                                                                                                                                                                                                                   
        createEvent: function(oEvent) {                                                                                                                                                                                                                        
            var oController = sap.ui.getCore().byId('onpremreportevent::TransOrderObjectPage').getController();                                                                                                                                                
            var sActionName = "com.sap.gateway.srvd.zkscctransorder.v0001.createDelay(com.sap.gateway.srvd.zkscctransorder.v0001.TransOrderType)";                                                                                                             
            var oContext = oEvent.getBinding().getBoundContext();                                                                                                                                                                                              
            var mParameter = {                                                                                                                                                                                                                                 
                contexts : oContext,                                                                                                                                                                                                                           
                label    : 'create Event'                                                                                                                                                                                                                      
            };                                                                                                                                                                                                                                                 
            oController.editFlow.invokeAction(sActionName,mParameter)                                                                                                                                                                                          
              .then( function (response){                                                                                                                                                                                                                      
                oContext.refresh();                                                                                                                                                                                                                            
            },function(erorr){                                                                                                                                                                                                                                 
                alert("error");                                                                                                                                                                                                                                
            });                                                                                                                                                                                                                                                
        }                                                                                                                                                                                                                                                      
    };                                                                                                                                                                                                                                                         
});                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               